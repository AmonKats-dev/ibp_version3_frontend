# Copy to .env and fill in your values

# ESLint / Build
DISABLE_ESLINT_PLUGIN=true
GENERATE_SOURCEMAP=false

# Railway backend â€“ replace with your backend's public URL from the Railway dashboard
# Example: https://ibp-api.up.railway.app (no trailing slash)
# The app appends /api/v1 and /api/admin to this base URL.
REACT_APP_API_URL=https://YOUR_RAILWAY_BACKEND_URL/api/v1
REACT_ADMIN_API_URL=https://YOUR_RAILWAY_BACKEND_URL/api/admin

# Frontend URL for static assets (e.g. when deployed on Vercel/Netlify)
REACT_APP_STATIC_URL=http://localhost:3000

# App config: ug | jm | mzb (optional, default: ug)
# REACT_APP_APP_CONFIG=ug
